<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.sj.myGomoku.dao.Dao">
	<insert id="signup">
     	
     <![CDATA[
        INSERT INTO USERS
        (
            USERINDEX,
            USERNAME,
            PASS      
        )
        VALUES
        (
          USERINDEX.NEXTVAL,
            #{username},
            #{password}
        )
    ]]>
	</insert>

</mapper>


